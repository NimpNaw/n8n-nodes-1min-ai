{
  "name": "n8n-nodes-1min-ai",
  "version": "1.0.0",
  "description": "n8n custom node for 1min.ai â€” compatible with the AI Agent node",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "1min.ai",
    "1minai",
    "ai",
    "llm",
    "langchain"
  ],
  "license": "MIT",
  "homepage": "https://github.com/NimpNaw/n8n-nodes-1min-ai",
  "author": {
    "name": "NimpNaw"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NimpNaw/n8n-nodes-1min-ai"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && mkdir -p dist/nodes/LmChat1MinAi dist/nodes/MinAiAudio dist/nodes/MinAiImage dist/credentials && cp nodes/LmChat1MinAi/1minai.svg dist/nodes/LmChat1MinAi/1minai.svg && cp nodes/MinAiAudio/1minai.svg dist/nodes/MinAiAudio/1minai.svg && cp nodes/MinAiImage/1minai.svg dist/nodes/MinAiImage/1minai.svg && cp nodes/LmChat1MinAi/1minai.svg dist/credentials/1minai.svg",
    "prepare": "npm run build",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lintfix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MinAiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/LmChat1MinAi/LmChat1MinAi.node.js",
      "dist/nodes/MinAiAudio/MinAiAudio.node.js",
      "dist/nodes/MinAiImage/MinAiImage.node.js"
    ]
  },
  "devDependencies": {
    "@langchain/core": "^0.1.0",
    "n8n-workflow": "^1.0.0",
    "@types/node": "^20.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "prettier": "^3.0.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.1.0",
    "n8n-workflow": ">=1.0.0"
  },
  "dependencies": {},
  "overrides": {
    "form-data": "^4.0.4"
  }
}
